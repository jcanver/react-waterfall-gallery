{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "semi": 0,
    "react/forbid-prop-types": 0,
    "no-nested-ternary": 0,
    "react/jsx-filename-extension": 0,
    "object-curly-newline": 0,
    "no-confusing-arrow": 0
  }
}